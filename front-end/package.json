    "easy-peasy": "^3.3.1",
    "firebase": "^7.15.5",
    "google-auth-library": "^6.0.2",
    "nanoid": "^3.1.10",
    "postcss-cli": "^7.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "tailwindcss": "^1.4.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.10.1",
    "@testing-library/react": "^10.3.0",
    "@testing-library/user-event": "^12.0.2",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
